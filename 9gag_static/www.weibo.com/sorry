(function() {
	var box = document.getElementById('reload_num_span');
	var time = parseInt(box.innerHTML, 10);
	function go() {
	box.innerHTML = time;
	time = time-1;
	if(time > 0) {
		setTimeout(go, 1000);
	}
	}
	go();
})();
</script>
